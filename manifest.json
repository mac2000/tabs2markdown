{
  "name": "tabs2markdown",
  "version": "2.0",
  "description": "Copies all open tabs into mardkown list of links",
  "icons": {
    "16": "images/md16.png",
    "32": "images/md32.png",
    "48": "images/md48.png",
    "128": "images/md128.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": ["clipboardWrite", "tabs", "notifications"],
  "manifest_version": 2
}
